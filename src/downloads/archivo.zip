<script>
    var spinnerVisible = false;
    function showDianLoading() {
        if (!spinnerVisible) {
            $("div#dian-loading").fadeIn("fast")
                .css("display", "flex");
            spinnerVisible = true;
        }
    }

    function hideDianLoading() {
        if (spinnerVisible) {
            var spinner = $("div#dian-loading");
            spinner.stop();
            spinner.fadeOut("fast");
            spinnerVisible = false;
        }
    }
</script>
